{
    "name": "@geometryzen/solid-js",
    "version": "0.9.12",
    "description": "solid-js as a system module",
    "type": "module",
    "exports": {
        ".": "./esm/es2015/index.min.js",
        "./html": "./esm/es2015/html/index.min.js",
        "./store": "./esm/es2015/store/index.min.js",
        "./universal": "./esm/es2015/universal/index.min.js",
        "./web": "./esm/es2015/web/index.min.js"
    },
    "files": [
        "esm",
        "system"
    ],
    "keywords": [
        "solid",
        "JavaScript"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup --config rollup.config.mjs",
        "check": "npx package-check",
        "clean": "rm -rf dist && rm -rf esm && rm -rf system && rm -rf coverage && rm -rf docs && rm -rf types",
        "coverage": "npm run test -- --coverage",
        "dev": "rollup --config rollup.config.ts -w",
        "docs": "npm run docs:typedoc && open-cli docs/index.html",
        "docs:typedoc": "rm -rf docs && typedoc --options typedoc.json",
        "lint": "eslint . --ext .ts",
        "pages": "npm run docs:typedoc && gh-pages -d docs",
        "pretest": "npm run build",
        "prepublishOnly": "pnpm build",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/geometryzen/solid-js.git"
    },
    "author": "David Geo Holmes",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/geometryzen/solid-js/issues"
    },
    "homepage": "https://github.com/geometryzen/solid-js#readme",
    "devDependencies": {
        "@arkweid/lefthook": "0.7.7",
        "@rollup/plugin-commonjs": "24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^10.0.1",
        "@skypack/package-check": "^0.2.2",
        "@types/jest": "^29.2.4",
        "@types/rollup-plugin-peer-deps-external": "2.2.1",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^5.47.1",
        "eslint": "^8.30.0",
        "gh-pages": "^4.0.0",
        "jest": "^29.3.1",
        "open-cli": "^7.1.0",
        "rollup-plugin-dts": "^5.1.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "solid-js": "^1.6.6",
        "ts-jest": "^29.0.3",
        "ts-jest-resolver": "^2.0.0",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.23",
        "typescript": "^4.9.4"
    }
}